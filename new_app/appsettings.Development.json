{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=aspnet-NewApp;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AppSettings": {
    "MySetting": "DevelopmentValue"
  },
  "CORS": {
    "AllowedOrigins": ["http://localhost:5000", "http://localhost:5001"]
  },
  "EnvironmentKeys": {
    "AzureBlobStorage": "UseDevelopmentStorage=true",
    "RedisCache": "localhost:6379"
  },
  "Endpoints": {
    "HealthCheck": "/health",
    "Swagger": "/swagger"
  }
}