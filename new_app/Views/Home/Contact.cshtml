@{
    ViewBag.Title = "Contact";
}

<main role="main" aria-labelledby="contact-title">
    <h1 id="contact-title">@ViewBag.Title</h1>
    <h3>@ViewBag.Message</h3>

    <p>
        Reach out to us at the following address:
    </p>
    <address>
        1234 Example Street<br>
        Example City, EX 12345
    </address>

    <p>
        For support, email us at 
        <a href="mailto:support@example.com">support@example.com</a><br>
        For marketing inquiries, email us at 
        <a href="mailto:marketing@example.com">marketing@example.com</a>
    </p>
</main>