{
  "ConnectionStrings": {
    "DefaultConnection": "${DefaultConnection_Env}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "Title": "${ApplicationTitle_Env}",
    "Environment": "${Environment_Env}"
  },
  "JwtSettings": {
    "Secret": "${JwtSecret_Env}",
    "Issuer": "${JwtIssuer_Env}",
    "Audience": "${JwtAudience_Env}",
    "ExpiryMinutes": "${JwtExpiryMinutes_Env}"
  }
}