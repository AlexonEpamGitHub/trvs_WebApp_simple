{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=aspnet-NewApp;Trusted_Connection=True;"
  },
  "FeatureFlags": {
    "EnableFeatureX": true
  },
  "ExternalServices": {
    "ServiceA": "https://api.servicea.com",
    "ServiceB": "https://api.serviceb.com"
  },
  "Authentication": {
    "JwtBearer": {
      "Authority": "https://localhost:5001",
      "Audience": "api1"
    }
  },
  "MailSettings": {
    "SmtpServer": "smtp.example.com",
    "Port": 587,
    "SenderEmail": "noreply@example.com"
  },
  "Localization": {
    "DefaultCulture": "en-US",
    "SupportedCultures": [ "en-US", "es-ES" ]
  },
  "Caching": {
    "Redis": {
      "ConnectionString": "localhost:6379"
    }
  }
}