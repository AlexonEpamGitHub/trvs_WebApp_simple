{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost;Initial Catalog=HotelReservationSystem;Integrated Security=True;Encrypt=False;TrustServerCertificate=True"
  },
  "WebSettings": {
    "ClientValidationEnabled": true,
    "UnobtrusiveJavaScriptEnabled": true
  },
  "EntityFramework": {
    "DefaultProvider": "System.Data.SqlClient"
  },
  "RuntimeOptions": {
    "TargetFramework": "net8.0"
  },
  "Newtonsoft.Json": {
    "DefaultContractResolver": "CamelCasePropertyNamesContractResolver",
    "DateFormatHandling": "IsoDateFormat",
    "NullValueHandling": "Ignore"
  }
}