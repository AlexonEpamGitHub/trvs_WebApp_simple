    <Content Include="wwwroot\css\bootstrap-grid.rtl.min.css" />
    <Content Include="wwwroot\css\bootstrap-reboot.css" />
    <Content Include="wwwroot\css\bootstrap-reboot.min.css" />
    <Content Include="wwwroot\css\bootstrap-reboot.rtl.css" />
    <Content Include="wwwroot\css\bootstrap-reboot.rtl.min.css" />
    <Content Include="wwwroot\css\bootstrap-utilities.css" />
    <Content Include="wwwroot\css\bootstrap-utilities.min.css" />
    <Content Include="wwwroot\css\bootstrap-utilities.rtl.css" />
    <Content Include="wwwroot\css\bootstrap-utilities.rtl.min.css" />
    <Content Include="wwwroot\css\bootstrap.css" />
    <Content Include="wwwroot\css\bootstrap.min.css" />
